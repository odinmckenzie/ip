{
    "name": "odin/ip",
    "version": "1.0.0",
    "description": "An IPv4 manipulation library in PHP.",
    "keywords": ["ip", "ipv4", "network", "subnet", "subnetting", "cidr", "supernetting", "calculator"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
          "name": "Odin McKenzie"
        }
      ],
    "autoload": {
        "psr-4": {
            "Odin\\IP\\": "src/"
        }
    },
    "scripts": {
        "tests": "vendor/bin/phpunit"
    },
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "<9.7"
    },
    "extra": {
        "_comment1": "phpunit 10.0-10.1 work with php >= 8.2 but in the cli there are deprecation messages",
        "_comment2": "phpunit >= 10.2 does not work with php >= 8.2"
    }
}